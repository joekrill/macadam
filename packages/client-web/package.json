{
  "name": "client-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^1.7.2",
    "@chakra-ui/react-utils": "^1.2.1",
    "@chakra-ui/system": "^1.8.2",
    "@chakra-ui/theme-tools": "^1.3.1",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@formatjs/cli": "^4.5.0",
    "@formatjs/intl-datetimeformat": "^4.2.6",
    "@formatjs/intl-displaynames": "^5.2.6",
    "@formatjs/intl-getcanonicallocales": "^1.8.0",
    "@formatjs/intl-listformat": "^6.3.6",
    "@formatjs/intl-locale": "^2.4.41",
    "@formatjs/intl-localematcher": "^0.2.21",
    "@formatjs/intl-numberformat": "^7.2.6",
    "@formatjs/intl-pluralrules": "^4.1.6",
    "@formatjs/intl-relativetimeformat": "^9.3.3",
    "@mdx-js/react": "^1.6.22",
    "@reduxjs/toolkit": "^1.7.0-rc.0",
    "@sentry/integrations": "^6.16.0",
    "@sentry/react": "^6.16.0",
    "@sentry/tracing": "^6.16.0",
    "@storybook/addon-a11y": "^6.4.8",
    "@storybook/addon-actions": "^6.4.8",
    "@storybook/addon-docs": "^6.4.8",
    "@storybook/addon-essentials": "^6.4.8",
    "@storybook/addon-links": "^6.4.8",
    "@storybook/node-logger": "^6.4.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.8",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/testing-library__jest-dom": "^5.14.2",
    "date-fns": "^2.27.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint-config-react-app": "^6.0.0",
    "favicons": "^6.2.2",
    "framer-motion": "^4.1.17",
    "history": "^5.1.0",
    "plausible-tracker": "^0.3.4",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.2",
    "react-icons": "^4.3.1",
    "react-intl": "^5.23.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "redux": "^4.1.2",
    "snakecase-keys": "^5.1.2",
    "source-map-explorer": "^2.5.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "web-vitals": "^2.1.2",
    "zod": "^3.11.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "analyze": "source-map-explorer build/static/js/*.js",
    "test": "react-scripts test --watchAll=false",
    "test:coverage": "yarn run test --collect-coverage",
    "test:watch": "react-scripts test --watchAll=true",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook -s public",
    "svg:optimize": "svgo -r src/",
    "typecheck": "tsc --noEmit",
    "i18n:extract": "formatjs extract 'src/**/*.ts*' --ignore '**/*.d.ts' --out-file translations/en.json",
    "i18n:compile": "formatjs compile-folder --ast translations src/features/i18n/messages",
    "i18n:compile:pseudo:enxa": "formatjs compile 'translations/en.json' --ast --pseudo-locale en-XA --out-file src/features/i18n/messages/en-XA.json",
    "i18n:compile:pseudo:enxb": "formatjs compile 'translations/en.json' --ast --pseudo-locale en-XB --out-file src/features/i18n/messages/en-XB.json",
    "i18n:compile:pseudo:xxls": "formatjs compile 'translations/en.json' --ast --pseudo-locale xx-LS --out-file src/features/i18n/messages/xx-LS.json",
    "i18n:compile:pseudo:xxac": "formatjs compile 'translations/en.json' --ast --pseudo-locale xx-AC --out-file src/features/i18n/messages/xx-AC.json",
    "i18n:compile:pseudo:xxha": "formatjs compile 'translations/en.json' --ast --pseudo-locale xx-HA --out-file src/features/i18n/messages/xx-HA.json",
    "i18n:compile:pseudo": "yarn run i18n:compile:pseudo:enxa && yarn run i18n:compile:pseudo:enxb && yarn run i18n:compile:pseudo:xxls && yarn run i18n:compile:pseudo:xxac && yarn run i18n:compile:pseudo:xxha",
    "i18n:update": "yarn run i18n:extract && yarn run i18n:compile && yarn run i18n:compile:pseudo",
    "favicons": "ts-node ./scripts/favicons.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/favicons": "^6.2.2",
    "@types/prettier": "^2.4.2",
    "@types/react-redux": "^7.1.20",
    "@types/svgo": "^2.6.0",
    "svgo": "^2.8.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.stories.{js,jsx,ts,tsx}"
    ]
  }
}
